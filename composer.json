{
    "name": "nosun/ueditor",
    "description": "UEditor extension for laravel-admin",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "extension",
        "ueditor"
    ],
    "homepage": "https://github.com/codingyu/UEditor",
    "license": "MIT",
    "authors": [
        {
            "name": "nosun",
            "email": "nosun2008@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.7",
        "nosun/laravel-ueditor": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Codingyu\\Ueditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codingyu\\Ueditor\\UeditorServiceProvider"
            ]
        }
    }
}
